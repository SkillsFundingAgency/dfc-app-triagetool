@model TriageToolOptionViewModel


<ol class="triage-result-list results-list">
    @foreach (var pageItem in Model.Pages)
    {
        <li class="dfc-code-search-resultitem triage-search-result-item info-card" id="@pageItem.">
            <div class="info-card-content">
                <h2 class="govuk-heading-m"><a class="govuk-link" href="@pageItem.Link">@pageItem.Title</a></h2>
                @if (!string.IsNullOrWhiteSpace(pageItem.Summary))
                {
                    @Html.Raw(pageItem.Summary)
                }
            </div>
        </li>
    }
</ol>
