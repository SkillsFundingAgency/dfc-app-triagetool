{
    "APPINSIGHTS_INSTRUMENTATIONKEY": "<<Key Here>>",
    "Logging": {
        "LogLevel": {
            "Default": "Debug"
        },
        "ApplicationInsights": {
            "LogLevel": {
                "Microsoft": "Trace"
            }
        }
    },
    "AllowedHosts": "*",
    "Configuration": {
        "ApplicationName": "DFC-App-TriageTool"
    },
    "Policies": {
        "HttpCircuitBreaker": {
            "DurationOfBreak": "00:01:00",
            "ExceptionsAllowedBeforeBreaking": 3
        },
        "HttpRetry": {
            "BackoffPower": 2,
            "Count": 3
        }
    },
    "Cms": {
      "TokenEndPointUrl": "Cms__TokenEndPointUrl",
      "GraphApiUrl": "Cms__GraphApiurl",
      "ClientId": "Cms__ClientId",
      "ClientSecret": "Cms__ClientSecret",
      "RedisCacheConnectionString": "Cms__RedisCacheConnectionString",
      "SqlApiUrl": "Cms__SqlApiUrl",
      "Expiry": "4"
    },
    "contentMode": {
        "contentMode": "PUBLISHED"
    }
}
